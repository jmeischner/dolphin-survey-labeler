{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Survey Label Exporter",
  "version": "0.0.1",
  "identifier": "com.pelatlas.survey-label-exporter",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Survey Label Exporter",
        "width": 1180,
        "height": 760,
        "minWidth": 960,
        "minHeight": 640
      }
    ]
  },
  "plugins": {
    "dialog": null,
    "shell": {
      "open": ".*"
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ]
  }
}
